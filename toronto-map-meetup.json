{
	"version": 8,
	"name": "osm",
	"metadata": {
		"maputnik:renderer": "mbgljs"
	},
	"glyphs": "https://geographyclub.github.io/toronto-map-meetup/assets/fonts/{fontstack}/{range}.pbf",
	"sprite": "https://geographyclub.github.io/toronto-map-meetup/assets/sprites/sprites",
	"sources": {
		"bing": {
			"type": "raster",
			"tiles": ["https://geographyclub.github.io/toronto-map-meetup/data/bing/{z}/{x}/{y}.png"],
			"tileSize": 256,
			"attribution": "&copy; Bing"
  },
		"osm": {
			"type": "vector",
			"url": "https://vector.openstreetmap.org/shortbread_v1/tilejson.json"
		},
		"roadway": {
			"type": "vector",
			"tiles": ["https://geographyclub.github.io/toronto-map-meetup/data/roadway/{z}/{x}/{y}.pbf"]
		}
	},
	"layers": [
		{
			"id": "background",
			"type": "background",
			"paint": {
				"background-color": "hsl(90, 29%, 70%)"
			}
		},
		{
			"id": "bing",
			"type": "raster",
			"source": "bing",
			"paint": {
				"raster-hue-rotate": 0,
				"raster-contrast": 0,
				"raster-saturation": 0.7,
				"raster-brightness-min": 0,
				"raster-brightness-max": 1
			}
		},
		{
			"id": "roadway",
			"type": "fill",
			"source": "roadway",
			"source-layer": "roadway",
			"paint": {
				"fill-color": "hsl(0, 0%, 100%)",
				"fill-opacity": 0.7
			}
		},
		{
			"id": "roadway-line",
			"type": "line",
			"source": "roadway",
			"source-layer": "roadway",
			"paint": {
				"line-color": "hsl(0, 0%, 0%)",
				"line-width": 1,
				"line-opacity": 1
			}
		}
	]
}
