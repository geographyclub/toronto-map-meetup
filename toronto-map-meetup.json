{
	"version": 8,
	"name": "osm",
	"metadata": {
		"maputnik:renderer": "mbgljs"
	},
	"glyphs": "https://geographyclub.github.io/toronto-map-meetup/assets/fonts/{fontstack}/{range}.pbf",
	"sprite": "https://geographyclub.github.io/toronto-map-meetup/assets/sprites/sprites",
	"sources": {
		"hybrid": {
			"type": "raster",
			"tiles": ["https://geographyclub.github.io/toronto-map-meetup/data/hybrid/{z}/{x}/{y}.png"],
			"tileSize": 256,
			"attribution": "Bing | Open Data Toronto",
			"maxzoom": 16
  },
		"osm": {
			"type": "vector",
			"url": "https://vector.openstreetmap.org/shortbread_v1/tilejson.json"
		}
	},
	"layers": [
		{
			"id": "background",
			"type": "background",
			"paint": {
				"background-color": "hsl(0, 0%, 100%)"
			}
		},
		{
			"id": "hybrid",
			"type": "raster",
			"source": "hybrid"
		}
	]
}
